<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.emilie.lostphone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Hub">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".applications.Messsages" />
        <activity android:name=".applications.Musique" />
        <activity android:name=".applications.Calculatrice" />
        <activity android:name=".applications.Meteo" />
        <activity android:name=".applications.Galerie" />
        <activity android:name=".applications.Plan" />
        <activity android:name=".applications.BlocNotes"></activity>
    </application>

</manifest>